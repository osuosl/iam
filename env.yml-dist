---
config:
  ganeti_clusters:
    - ganeti
  chef:
    host: chef.osuosl.org/organizations/osuosl
    client: test_chef_client
    key_path: ./test_chef_client.pem
  db_url: sqlite:///tmp/development.sqlite
  cache_path: ./tmp
  log_file_path: logfile
  db_collector_dbs:
    - type: postgres
      user: test_db_user
      password: test_db_pass
      host: testing-psql
    - type: mysql
      user: root
      password: toor
      host: testing-mysql
